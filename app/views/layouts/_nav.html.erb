<div class="flex flex-row w-full justify-end py-3 gap-x-8 items-center">
	<%= button_to "Leaderboard", root_path, class: "text-black font-bold", data: { turbo_frame: "page", turbo_action: "advance" }, aria: { label: "leaderboard"}, method: :get%>
	<% if current_user.role == "label_admin" then %>
			<%= button_to "Admin", admin_root_path, class: "text-black font-bold", data: { turbo_frame: "page", turbo_action: "advance" }, aria: { label: "admin"}, method: :get%>
	<% end %>
	<%= button_to "Account", account_path, class: "text-black font-bold", aria: { label: "account"}, method: :get %> 
	<%= button_to "Sign out", destroy_user_session_path, class: "text-black font-bold", aria: { label: "sign out"}, method: :delete %> 
</div>